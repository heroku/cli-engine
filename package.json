    "lodash.maxby": "4.x",
    "lodash.uniqby": "4.x",
    "log-chopper": "^1.0.1",
    "moment": "^2.18.1",
    "netrc-parser": "2.0.2",
    "node-gyp": "3.x",
    "rwlockfile": "^1.4.5",

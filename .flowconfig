[ignore]
<PROJECT_ROOT>/tmp/.*
<PROJECT_ROOT>/lib/.*
<PROJECT_ROOT>/test/roots/.*
.*/node_modules/nock.*

[include]

[libs]

[options]
emoji=true
unsafe.enable_getters_and_setters=true
suppress_comment= \\(.\\|\n\\)*flow\\$ignore
module.use_strict=true
module.ignore_non_literal_requires=true
suppress_type=$FlowFixMe
